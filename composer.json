{
  "name"        : "cti/rest-client-bundle",
  "type"        : "symfony-bundle",
  "description" : "Symfony bundle supplying REST client to be reused across various applications",
  "keywords"    : [ "rest", "rest client" ],
  "license"     : "MIT",
  "homepage"    : "https://github.com/lolaent/rest-client-bundle",
  "authors"     : [
    {
      "name"  : "Georgiana Gligor",
      "email" : "gb@tekkie.ro"
    },
    {
      "name"  : "Alex Cos",
      "email" : "alexandru.cos@cloudtroopers.com"
    }
  ],
  "require" : {
    "php"                : ">=5.3.0",
    "guzzlehttp/guzzle"  : "3.*",
    "jms/serializer-bundle" : "0.13.0",
    "misd/guzzle-bundle"    : "~1.1"
  },
  "require-dev" : {
    "phpunit/phpunit" : "4.2.*",
    "symfony/framework-bundle": ">=2.3.0"
  },
  "autoload" : {
    "psr-0": { "CTI\\RestClientBundle": "" }
  },
  "target-dir" : "CTI/RestClientBundle"
}
